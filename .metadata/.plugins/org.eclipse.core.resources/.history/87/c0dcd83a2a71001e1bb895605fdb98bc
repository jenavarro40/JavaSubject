
public class Asgn02 {

	public static void main(String[] args) {
		Question ques=new Question();
		ques.readNumberArray();
		Object [] QuestionExit=ques.StoreQuestions();
		String [][] questions=new String[ques.getNumberQuestion()][ques.getNumberLinesPerQuestion()];
		int [] answer=new String[ques.getNumberLinesPerQuestion()];
		questions=(String[][])QuestionExit[0];
		answer=(int[])QuestionExit[1];

	}

}
